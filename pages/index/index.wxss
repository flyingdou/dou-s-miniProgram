page{
  width: 100%;
  height: 100%;
  background: #f0f2f2;
}

.userInfo{
  margin-top: 20rpx;
  height: auto; 
  background: white;
  padding: 20rpx;
  text-align: center;
}

.imageDiv{
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}

.image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow:hidden;
  display:block;
}

.connectWifiDiv{
  margin-top: 20rpx;
  height: 100%;
  background: white;
  padding: 20rpx;
}

.connectWifi{
  background: green;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin: 0 auto;
  color: white;
  font-size: 28rpx;
  line-height: 120rpx;
  text-align: center;
}